{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "cliOptions": {
    "exclude": [
      "./util/submit_rings.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-bitwise": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "max-line-length": [true, {"limit": 120, "ignore-pattern": "^import |^export {(.*?)}|class [a-zA-Z]+ implements |//"}]
  },
  "rulesDirectory": []
}
