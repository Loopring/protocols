{
  "name": "loopring_v2.js",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.12"
  },
  "description": "Loopring protocol v2 JS Library",
  "main": "dist/loopring_v2.umd.js",
  "module": "dist/loopring_v2.es5.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "start": "rollup -c rollup.config.ts",
    "build": "yarn tsc -b",
    "build:ci": "yarn build",
    "clean": "shx rm -rf lib",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Loopring/protocols.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Loopring/protocols/issues"
  },
  "homepage": "https://github.com/Loopring/protocols/packages/loopring_v2.js/packages/v2.js/README.md",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^24.5.0",
    "jest-config": "^24.5.0",
    "lint-staged": "^8.1.5",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.16.4",
    "rollup": "^1.6.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "semantic-release": "^15.13.3",
    "shx": "^0.2.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "3.0.1"
  },
  "dependencies": {
    "@loopring_v2/contract-addresses": "^1.0.0",
    "@loopring_v2/contract-artifacts": "^1.0.0",
    "@loopring_v2/contract-wrapper": "^1.0.0",
    "@loopring_v2/json-schemas": "^1.0.0",
    "@loopring_v2/order-utils": "^1.0.0",
    "@loopring_v2/types": "^1.0.0",
    "@types/web3-provider-engine": "^14.0.0",
    "ethereum-types": "^2.1.0",
    "lodash": "^4.17.11"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --no-semi",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "precommit": "lint-staged"
}
