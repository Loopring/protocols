{
  "compilerOptions":
  {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2017", "dom"],
    "strict": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "noImplicitReturns": true,
    "pretty": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": [],
  "references": [
  {
    "path": "./packages/contract-artifacts"
  },
  {
    "path": "./packages/v2.js",
    "path": "./packages/types",
    "path": "./packages/json-schemas",
    "path": "./packages/order-utils",
    "path": "./packages/contract-addresses",
    "path": "./packages/contract-artifacts",
    "path": "./packages/contract-wrapper"
  }]
}
