{
    "parser": "babel-eslint",
    "rules":
    {
        "max-len": 0,
        "comma-dangle": 0,
        "brace-style": [2, "stroustrup"],
        "no-console": 0,
        "padded-blocks": 0,
        "indent": [2, 2,
        {
            "SwitchCase": 1
        }],
        "spaced-comment": 1,
        "quotes": ["error", "single",
        {
            "allowTemplateLiterals": true
        }],
        "import/prefer-default-export": "off",
        "arrow-parens": 0,
        "consistent-return": 0,
        "no-useless-escape": 0,
        "no-underscore-dangle": 0
    },
    "extends": "airbnb-base",
    "env":
    {
        "browser": true,
        "jest": true
    }
}