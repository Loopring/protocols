{
  "name": "loopring_v3",
  "version": "3.0.0",
  "description": "Loopring DEX protocol version 3.0",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+http://github.com/Loopring/protocols.git"
  },
  "watch": {
    "compile": {
      "patterns": [
        "contracts"
      ],
      "extensions": "sol",
      "delay": 10000
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run formatc"
    }
  },
  "scripts": {
    "watch": "npm-watch compile",
    "transpile": "rm -rf ./transpiled; copyfiles ./build/**/* ./transpiled && tsc && npm run genabi",
    "test": "npm run compile && npm run transpile && node --max-old-space-size=4096 `which truffle` test --network development",
    "docker": "docker-compose up --build --abort-on-container-exit; docker-compose logs -f test",
    "testdocker": "npm run transpile && truffle test --network docker",
    "compile": "truffle compile",
    "make": "LIBRARY_PATH=$LIBRARY_PATH:/usr/local/lib/ make",
    "build": "./install && LIBRARY_PATH=$LIBRARY_PATH:/usr/local/lib/ make && npm run compile",
    "genabi": "rm -rf ./ABI && mkdir -p ABI/version36 && node ./genAbi.js",
    "ganache": "./ganache.sh",
    "migrate": "npm run transpile && truffle migrate --network live | tee -a mainnet.deployed",
    "migrate-ropsten": "npm run transpile && truffle migrate --network ropsten   | tee -a ropsten.deployed",
    "migrate-rinkeby": "npm run transpile && truffle migrate --network rinkeby   | tee -a rinkeby.deployed",
    "migrate-goerli": "npm run transpile && truffle migrate --network goerli   | tee -a goerli.deployed",
    "migrate-dev": "npm run transpile && truffle migrate --network development   | tee -a development.deployed",
    "coverage": "npm run transpile && node --max-old-space-size=4096 `which truffle` run coverage",
    "truffle": "truffle",
    "solium": "solium -d contracts/",
    "formatc": "clang-format -i circuit/Gadgets/* circuit/Utils/* circuit/Circuits/* circuit/main.cpp",
    "clean": "rm -rf build blocks keys transpiled",
    "format-circuits": "git-clang-format",
    "v": "node -v",
    "preinstall": "rm -rf node_modules/websocket/.git",
    "test-circuits": "./build/circuit/dex_circuit_tests",
    "testc": "npm run test-circuits"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "3.5.33",
    "@types/lodash": "^4.14.139",
    "@types/mocha": "^8.0.0",
    "@types/node": "15.6.1",
    "@types/request-promise-native": "^1.0.17",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "ajv": "^6.10.0",
    "chai": "^4.2.0",
    "clang-format": "^1.4.0",
    "copyfiles": "^2.1.0",
    "diff": ">=3.5.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "ethlint": "^1.2.5",
    "ganache-cli": "6.9.1",
    "husky": "^4.0.4",
    "mocha": "^8.0.1",
    "node": "^13.10.1",
    "npm": "^6.14.8",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "solc": "0.7.3",
    "solidity-coverage": "^0.7.5",
    "solium": "^1.2.5",
    "truffle": "^5.1.45",
    "truffle-assertions": "^0.9.1",
    "truffle-flattener": "^1.5.0",
    "types-bn": "0.0.1",
    "typescript": "4.0.5",
    "webpack": "^5.1.2"
  },
  "dependencies": {
    "@freemanz/ts-utils": "^0.2.0",
    "@types/bitwise-xor": "0.0.31",
    "@types/eth-sig-util": "^2.1.0",
    "@types/web3": "^1.0.20",
    "any-promise": "1.3.0",
    "axios": "^0.21.1",
    "bignumber.js": "^4.1.0",
    "bitwise-xor": "0.0.0",
    "blake-hash": "^2.0.0",
    "blake2b": "^2.1.3",
    "bluebird": "^3.5.1",
    "bn.js": "^5.0.0",
    "braces": "^3.0.2",
    "chalk": "^3.0.0",
    "dotenv": "^8.0.0",
    "es6-iterator": "^2.0.3",
    "es6-map": "^0.1.5",
    "es6-promisify": "^6.0.2",
    "escape-string-regexp": "^2.0.0",
    "eslint-utils": "^1.4.2",
    "estraverse": "^4.2.0",
    "esutils": "^2.0.2",
    "eth-json-rpc-filters": "^4.2.1",
    "eth-sig-util": "^2.5.3",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^6.2.0",
    "event-emitter": "^0.3.5",
    "events": "^3.0.0",
