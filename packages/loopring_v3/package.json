    "find-config": "^1.0.0",
    "js-sha3": "^0.8.0",
    "lodash": "^4.17.19",
    "loopringV3.js": "file:../loopring_v3.js/",
    "node-bin-setup": "^1.0.6",
    "sha2": "^1.0.2",
    "sha3": "^2.0.0",
    "snarkjs": "^0.3.33",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.3.5",
    "truffle-privatekey-provider": "^1.3.0",
    "typedarray-to-buffer": "^3.1.5",
    "web3": "^1.2.6"
  }
}
